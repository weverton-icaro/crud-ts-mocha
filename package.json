{
  "name": "teste-info",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn test && tsc",
    "start": "yarn dev",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --pretty --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migrate:create": "ts-node ./node_modules/typeorm/cli migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli migration:revert",
    "seed:users": "ts-node-dev src/shared/infra/typeorm/seed/seederUsers.ts",
    "seed:cars": "ts-node-dev src/shared/infra/typeorm/seed/seederCars.ts",
    "test": "mocha --reporter spec --importHelpers ts-node/register test/**/*.spec.ts",
    "coverage": "nyc yarn test"
  },
  "keywords": [
    "mocha",
    "chai"
  ],
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.13",
    "@types/nodemailer": "^6.4.5",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esm": "^3.2.25",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/chai-http": "^4.2.0",
    "@types/expect": "^24.3.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "class-transformer": "^0.5.1",
    "csv-parse": "^5.3.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.0.0",
    "moment-timezone": "^0.5.37",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.2.41"
  }
}
